# VisionEval.cnf for VERSPM with "vertical" stages by Year
# Each year runs as a separate stage; clone stage-year-2030 with
#   different inputs to create additional scenarios.

# Model Structural Parameters
ScriptsDir  : scripts
InputDir    : inputs

# Model description
Seed        : 1
Model       : VERSPM 3.0 ("Vertical" stages/Years)
Region      : RVMPO
State       : OR
ModelStages :
    "stage-year-2010":
      Dir: stage-year-2010
      Reportable: true
    "stage-year-2038":
      Dir: stage-year-2038
      Reportable: true
